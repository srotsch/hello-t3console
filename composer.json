{
  "name": "srotsch/hello-t3console",
  "description": "An example of extending TYPO3 Console with a custom command controller",
  "license": "GPL-2.0+",
  "type": "typo3-console-command",
  "authors": [
    {
      "name": "Stefan Rotsch",
      "email": "srotsch@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Srotsch\\HelloT3console\\": "src/"
    }
  },
  "require": {
    "php": ">=5.6",
    "helhum/typo3-console": "^4.0"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "web"
    },
    "helhum/typo3-console": {
      "install-extension-dummy": "0"
    }
  }
}
